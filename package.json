  "license": "MIT",
  "dependencies": {
    "npm-run-all": "^4.1.5"
  },
  "scripts": {
    "install:frontend": "yarn --cwd=frontend install",
    "install:backend": "yarn --cwd=backend install",
    "dev:frontend": "yarn --cwd=frontend dev",
    "dev:backend": "yarn --cwd=backend dev",
    "build:frontend": "yarn --cwd=frontend build",
    "build:backend": "yarn --cwd=backend build",
    "preview:frontend": "yarn --cwd=frontend preview",
    "preview:backend": "yarn --cwd=backend start",
    "postinstall": "run-p install:*",
    "dev": "run-p dev:*",
    "build": "run-s build:*",
    "preview": "run-p preview:*"
  }
}
